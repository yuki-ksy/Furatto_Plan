スポット新規登録

<%= form_with model: @tourism, local: true do |f| %>

<%= f.attachment_field :images_images, multiple: true %>

<%= f.text_field :name, class: "form-control",autofocus: true, autocomplete: "name", placeholder: "スポット名" %>

<%= f.text_field :production, class: "form-control",autofocus: true, autocomplete: "production", placeholder: "スポット紹介" %>

<%= f.select :prefecture_id, Prefecture.prefectures, {prompt: '選択してください'}, class: 'form-control btn btn-info' %>

<%= f.select :genre_id, Genre.genres, {prompt: '選択してください'}, class: 'form-control btn btn-info' %>

<%= f.text_field :budget, class: "form-control",autofocus: true, autocomplete: "budget", placeholder: "予算" %>円

<%= f.submit '新規登録', class: "btn btn-primary btn-block" %>

<% end %>
